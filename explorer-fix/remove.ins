[Initial]
Message=Deinstalliere Explorer-Fix
DefVar $ExitCode$

[Aktionen]
ShowBitmap "%ScriptPath%\logo.png" "Explorer-Fix"
Files_Setup
Sub_HandleExitCode

[Files_Setup]
delete "%CommonStartupDir%\start_explorer.cmd"
delete "%CommonStartupDir%\Powershell" -f

[Sub_HandleExitCode]
; check return code
Set $ExitCode$ = GetLastExitCode
comment "GetLastExitCode: " + $ExitCode$
if not($ExitCode$ = "0")
  LogError "Fatal: setup returned exit code " + $ExitCode$
  IsFatalError
endif